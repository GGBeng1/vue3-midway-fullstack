{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "./src"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "vite/client",
      "element-plus/global",
      "unplugin-icons/types/vue",
      "vue/macros-global"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "skipLibCheck": true,
    "preserveValueImports": false
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}